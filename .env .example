# .env file for Helix Platform
##############################
WEBUI_SECRET_KEY=webuipassword123
NGROK_URL=https://2a2642ee6876.ngrok-free.app
# PostgreSQL Credentials
POSTGRES_DB=helix_db
POSTGRES_USER=helix_user
