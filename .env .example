N8N_CORS_ALLOW_ORIGINS=*
NODE_ENV=production
GENERIC_TIMEZONE=UTC
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin
N8N_CORS_ALLOWED_ORIGINS=http://0.0.0.0:8088,http://localhost:8088,https://2a2642ee6876.ngrok-free.app
N8N_CORS_ALLOW_HEADERS=Authorization,Keep-Alive,User-Agent,Content-Type
N8N_CORS_ALLOW_METHODS=GET,POST,PUT,DELETE,OPTIONS

